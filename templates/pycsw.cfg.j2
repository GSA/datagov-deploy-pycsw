[defaults]
ckan_url={{ pycsw_catalog_ckan_url }}

[server]
home=/home/pycsw/current
url={{ pycsw_app_url }}
mimetype=application/xml; charset=UTF-8
encoding=UTF-8
language=en-US
maxrecords=500
#loglevel=DEBUG
#logfile=/tmp/pycsw.log
#ogc_schemas_base=http://foo
#federatedcatalogues=http://catalog.data.gov/csw
#pretty_print=true
gzip_compresslevel=9
#domainquerytype=range
#domaincounts=true
#spatial_ranking=true
profiles=apiso

[manager]
transactions=false
allowed_ips=127.0.0.1
#csw_harvest_pagesize=10
 
[metadata:main]
identification_title=CSW interface for catalog.data.gov
identification_abstract= {{ pycsw_abstract }}
identification_keywords_type=theme
identification_fees=None
identification_accessconstraints=None
provider_name=U.S. General Services Administration
provider_url=https://www.gsa.gov
contact_name=Data.gov Administrator
contact_position=Data.gov Site Administrator
contact_address=1800 F St NW
contact_city=Washington
contact_stateorprovince=DC
contact_postalcode=20405
contact_country=USA
contact_phone=(800)-488-3111
contact_fax=DSN 465-1416
contact_email={{ pycsw_support_email }}
contact_url=https://data.gov/contact
contact_hours=9:00am - 4:30pm ET
contact_instructions=Preferred method through email {{ pycsw_support_email }} or contact URL https://data.gov/contact
contact_role=publisher,custodian
 
[repository]
db_user = {{ pycsw_db_user }}
db_pass = {{ pycsw_db_password }}
db_server = {{ pycsw_db_host }}
db_database = {{ pycsw_db_name }}
database=postgresql://%(db_user)s:%(db_pass)s@%(db_server)s/%(db_database)s
table=records
{% if pycsw_filter is defined %}
filter={{ pycsw_filter }}
{% endif %}

[metadata:inspire]
enabled=true
languages_supported=eng,gre
default_language=eng
date=YYYY-MM-DD
gemet_keywords=Utility and governmental services
conformity_service=notEvaluated
contact_name=Organization Name
contact_email=Email Address
temp_extent=YYYY-MM-DD/YYYY-MM-DD

